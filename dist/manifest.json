{
	"manifest_version": 3,
	"name": "Twitter filtered",
	"version": "0.0.0",

	"description": "Wipe bluechecks from Twitter replies",

	"browser_specific_settings": {
		"gecko": {
			"id": "gut@milesseventh.com"
		}
	},

	"icons": {
		"32": "graphics/32.png",
		"96": "graphics/96.png",
		"128": "graphics/128.png"
	},

	"permissions": [
	],

	"host_permissions": [
		"https://corsproxy.io/*"
	],

	"content_scripts": [{
		"matches": ["*://*.twitter.com/*"],
		"js": ["js/twitter.js"]
	}],

	"options_ui": {
		"page": "options/options.html"
	}
}